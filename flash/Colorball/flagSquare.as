package {	import flash.display.Sprite;	import flash.events.Event;		public class flagSquare extends Sprite	{		private var _xPos:int;		private var _yPos:int;		private var _widthSquare:int;		private var _heightSquare:int;		private var _color:uint;				private var _newBlurFilter:Blurfilter = new Blurfilter(2, 2, 1, this);				public function flagSquare(xPos:int, yPos:int, widthSquare:int, heightSquare, color:uint)		{			this._xPos = xPos;			this._yPos = yPos;			this._widthSquare = widthSquare;			this._heightSquare = heightSquare;			this._color = color;						drawSquare();			setPosition();		}		private function drawSquare():void		{			this.graphics.beginFill(0x000000, 0);			this.graphics.lineStyle(2, this._color);			this.graphics.drawRect(0, 0, this._widthSquare, this._heightSquare);			this.graphics.endFill();		}				private function setPosition():void		{			this.x = this._xPos;			this.y = this._yPos;		}				public function drawActiveSquare():void		{			this.graphics.beginFill(this._color, 0.3);			this.graphics.lineStyle(2, this._color);			this.graphics.drawRect(0, 0, this._widthSquare, this._heightSquare);			this.graphics.endFill();		}	}	}