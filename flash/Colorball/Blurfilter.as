package  {	import flash.filters.BlurFilter;		/**	 * ...	 * @author J. Hoogerwerf	 */	public class Blurfilter	{		public function Blurfilter(xDrag:Number, yDrag:Number, quality:int, obj:Object) 		{			//APPLY BLUR FILTER			var myTempFilters: Array = obj.filters;			var myBlur: BlurFilter = new flash.filters.BlurFilter(xDrag, yDrag, quality);			myTempFilters.push(myBlur);			obj.filters = myTempFilters;		}			}}